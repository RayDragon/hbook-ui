{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "QNTN Serve Local",
      "type": "shell",
      "command": "ng serve qntn --port 4202",
      "isBackground": true,
      "problemMatcher": [
        {
          "pattern": [
            {
              "regexp": ".",
              "file": 1,
              "location": 2,
              "severity": 3,
              "code": 4,
              "message": 5
            }
          ],
          "background": {
            "activeOnStart": true,
            "beginsPattern": ".",
            "endsPattern": "."
          }
        }
      ]
    }
  ]
}
